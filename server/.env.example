# Server Configuration
PORT=3001

# External Database API URL
EXTERNAL_DB_URL=https://your-external-db-url.com

# External Database Cookies (Required if the API is behind authentication)
# Open browser dev tools -> Network -> Request -> Copy 'Cookie' header value
EXTERNAL_DB_COOKIES=JSESSIONID=xyz; other_cookie=abc

# Google Gemini API Key
# Get your API key from: https://makersuite.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# Environment (development, production, test)
NODE_ENV=development

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# CORS Allowed Origins (comma-separated)
CORS_ALLOWED_ORIGINS=http://localhost:3000,http://localhost:3001
